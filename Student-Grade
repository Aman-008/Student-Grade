#include <iostream>
using namespace std;

// Note :- Hum isme Single string ('' ) ka use karte hai aur isme 'cout' ki jagaj return likhna padata hai
char findGrade(int marks)
{
    switch (marks / 10)
    {
    case 10:
        return 'A';
        break;

    case 9:

        return 'A';
        break;

    case 8:
        return 'B';
        break;

    case 7:
        return 'C';
        break;

    case 6:
        return 'D';
        break;
    default:
        return 'E';
    }
    // if (marks >= 90)
    // {
    //     return 'A';
    // }
    // else if (marks >= 80)
    // {
    //     return 'B';
    // }
    // else if (marks >= 70)
    // {
    //     return 'C';
    // }
    // else if (marks >= 60)
    // {
    //     return 'D';
    // }
    // else
    // {
    //     return 'E';
    // }
}
int main()
{
    // int marks;
    // cout << "Enter Your Marks : ";
    // cin >> marks;
    // char grade = findGrade(marks);
    // cout << grade << endl;
    for (int i = 0; i <= 100; i++)
    {
        char grade = findGrade(i);
        cout << "Grade for marks = " << i << " is " << grade << endl;
    }
    
    return 0;
}
